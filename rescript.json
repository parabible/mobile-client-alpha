{
  "name": "rescript-client",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "rescript-webapi",
    "@glennsl/rescript-fetch",
    "@glennsl/rescript-json-combinators"
  ],
  "bsc-flags": [
    "-open RescriptCore",
    "-open JsonCombinators"
  ],
  "jsx": {
    "version": 4
  }
}